{
  "name": "ontyme",
  "version": "1.0.0",
  "description": "A centralized debouncer for any application written in TypeScript",
  "main": "dist/node/index.js",
  "source": "src/index.ts",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": "./dist/node",
    "./web": "./dist/web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theclocker/ontyme.git"
  },
  "scripts": {
    "build:node": "webpack --config ./webpack.config.node.js --mode=development",
    "build:web": "webpack --config ./webpack.config.web.js --mode=production"
  },
  "author": "Jonathan Vega",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theclocker/ontyme/issues"
  },
  "homepage": "https://github.com/theclocker/ontyme#readme",
  "devDependencies": {
    "@parcel/transformer-typescript-types": "^2.3.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
